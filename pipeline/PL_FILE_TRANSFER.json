{
	"name": "PL_FILE_TRANSFER",
	"properties": {
		"description": "from upload to processing folder",
		"activities": [
			{
				"name": "fileTransfer",
				"description": "file transfering from upload container to processing container on data lake.",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"deleteFilesAfterCompletion": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "dls_upload_binary",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "dls_processing_binary",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2021-07-07T09:31:28Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}