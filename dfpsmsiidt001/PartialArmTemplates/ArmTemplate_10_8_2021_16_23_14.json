												"type": "String"
											},
											"MociTransactionTypeEnglishName": {
												"value": {
													"value": "@activity('LookupColumns').output.value.PSMSMOCITRANSACTIONTYPES",
													"type": "Expression"
												},
												"type": "String"
											},
											"IsInitialLoad": {
												"value": {
													"value": "@activity('LookupInitialLoad').output.firstrow.lkp",
													"type": "Expression"
												},
												"type": "Single"
											}
										}
									},
