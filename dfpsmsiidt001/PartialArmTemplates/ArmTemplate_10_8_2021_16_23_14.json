											"CrNumber": {
												"value": {
													"value": "@activity('LookupColumns').output.value.COMPANYCR",
													"type": "Expression"
												},
												"type": "String"
											},
											"EntityTransactionTypeCode": {
												"value": {
													"value": "@activity('LookupColumns').output.value.COMPANYTRANSACTIONTYPE",
													"type": "Expression"
												},
												"type": "String"
											},
											"EntityTransactionTypeNameArabic": {
												"value": {
													"value": "@activity('LookupColumns').output.value.COMPANYTRANSACTIONTYPEARABIC",
